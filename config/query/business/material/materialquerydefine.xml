    <name value="getMinPriceByMaterialId" /> 
    <where>
    <w name="a1.materialId" des="" reqName="materialid" dtype="ch" oper="="  lg="AND" bt="" />
      </where>
    <select>
      <f name="a1.materialId" alias="" ctype="" />
