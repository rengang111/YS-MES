    <from>b_pricesupplier a1, (SELECT min(REPLACE(a.price,',','')+0) price,a.materialId FROM b_pricesupplier a GROUP BY a.materialId) a2 WHERE a1.materialId = a2.materialId AND a2.price = REPLACE(a1.price,',','') </from>
