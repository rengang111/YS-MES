    </select>
    <!-- 条件可以获得系统变量值 '@:来源session,#来源 2016/1/8未实现' -->
    <!-- $：由程序根据where句的情况追加where关键字' 无$：程序不负责追加where关键字，在from句中需写入where关键字 -->
    <from>b_MouldPayList as A WHERE A.DeleteFlag = '0' </from>
    <orderby>payTime</orderby>
    <groupby></groupby>
    <having></having>
	<power>
	</power>    
  </query>
