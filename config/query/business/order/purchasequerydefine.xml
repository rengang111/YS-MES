<query>
    <pool></pool>    
    <name value="getRawRequirement" />
    <where>
    	<w name="bomId" des="" reqName="bomId" dtype="ch" oper="=" lg="AND" bt="" />
    </where>
  	<select>
      <f name="a.recordId" alias="" ctype="" />
