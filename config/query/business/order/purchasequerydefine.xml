    <groupby></groupby>
    <having></having>
	<power>
	</power>    
  </query>
<query>
    <pool></pool>    
    <name value="getRawRequirement" />
    <where>
    	<w name="bomId" des="" reqName="bomId" dtype="ch" oper="=" lg="AND" bt="" />
    </where>
  	<select>
      <f name="a.recordId" alias="" ctype="" />
      <f name="a.materialRecordId" alias="" ctype="" />
      <f name="a.parentId" alias="" ctype="" />
      <f name="a.subId" alias="" ctype="" />
      <f name="a.bomId" alias="" ctype="" />
      <f name="a.materialId" alias="" ctype="" />
      <f name="a.materialName" alias="" ctype="" />
      <f name="b.rawMaterialId" alias="" ctype="" />
      <f name="b.rawMaterialName" alias="" ctype="" />
      <f name="b.unit" alias="" ctype="" />
      <f name="b.zzunit" alias="" ctype="" />
      <f name="b.unit" alias="unitName" ctype="计量单位" />
      <f name="b.zzunit" alias="zzunitName" ctype="计量单位" />
      <f name="format(sum((REPLACE(IFNULL(a.quantity, 0),',','')  * 2000 * b.weight) ),2)" alias="requirement" ctype="" />
  </select>
    <!-- 条件可以获得系统变量值 '@:来源session,#来源 2016/1/8未实现' -->
    <!-- $：由程序根据where句的情况追加where关键字' 无$：程序不负责追加where关键字，在from句中需写入where关键字 -->
    <!-- 限定显示订单状态为"BOM表已做成,已评审"的数据 -->
   <from> v_bomdetaillist a LEFT JOIN v_zzrawmaterial b ON a.materialId = b.materialId WHERE a.deleteflag = '0'  AND a.supplierId = '0574YS00' </from>
    <orderby>b.rawMaterialId</orderby>
    <groupby>b.rawMaterialId</groupby>
    <having></having>
	<power></power>    
  </query>
<query>
    <pool></pool>    
    <name value="getRequirement" />
    <where>
    	<w name="a.bomId" des="" reqName="bomId" dtype="ch" oper="=" lg="AND" bt="" />
    </where>
  	<select>
      <f name="a.recordId" alias="" ctype="" />
      <f name="a.materialRecordId" alias="" ctype="" />
      <f name="a.materialParentId" alias="" ctype="" />
      <f name="a.bomId" alias="" ctype="" />
      <f name="a.materialId" alias="" ctype="" />
      <f name="a.materialName" alias="" ctype="" />
      <f name="a.rawMaterialId" alias="" ctype="" />
      <f name="a.rawMaterialName" alias="" ctype="" />
      <f name="IFNULL(a.quantity,0)" alias="quantity" ctype="" />
      <f name="IFNULL(a.weight,0)" alias="weight" ctype="" />
      <f name="a.matunit" alias="" ctype="计量单位" />
      <f name="IFNULL(a.convertUnit,0)" alias="convertUnit" ctype="" />
      <f name="IFNULL(a.unit,a.matunit)" alias="matunitName" ctype="计量单位" />
      <f name="a.zzunit" alias="" ctype="" />
      <f name="a.unit" alias="unitName" ctype="计量单位" />
      <f name="a.zzunit" alias="zzunitName" ctype="计量单位" />
      <f name="IFNULL(a.zzunit,a.unit)" alias="chgUnitName" ctype="计量单位" />
      <f name="IFNULL(d.price,0)" alias="price" ctype="" />
      <f name="d.supplierId" alias="" ctype="" />
      <f name="IFNULL(b.lastPrice,0)" alias="lastPrice" ctype="" />
      <f name="b.lastSupplierId" alias="" ctype="" />
      <f name="IFNULL(b.minPrice,0)" alias="minPrice" ctype="" />
      <f name="b.minSupplierId" alias="minSupplierId" ctype="" />
      <f name="IFNULL(c.lastPrice,0)" alias="matLastPrice" ctype="" />
      <f name="c.lastSupplierId" alias="matLastSupplierId" ctype="" />
      <f name="c.minPrice" alias="matMinPrice" ctype="" />
      <f name="c.minSupplierId" alias="matMinSupplierId" ctype="" />
  </select>
    <!-- 条件可以获得系统变量值 '@:来源session,#来源 2016/1/8未实现' -->
    <!-- $：由程序根据where句的情况追加where关键字' 无$：程序不负责追加where关键字，在from句中需写入where关键字 -->
    <!-- 限定显示订单状态为"BOM表已做成,已评审"的数据 -->
   <from> v_bomdetailraw a LEFT JOIN v_supplierprice2 b ON a.rawMaterialId = b.materialId LEFT JOIN v_supplierprice2 c ON a.materialId = c.materialId LEFT JOIN b_pricesupplier d ON a.materialId = d.materialId AND a.supplierId = d.supplierId</from>
    <orderby>a.materialId,a.rawMaterialId</orderby>
    <groupby></groupby>
    <having></having>
	<power></power>    
  </query>
<query>
    <pool></pool>    
    <name value="getRequirementPrice" />
    <where>
    	<w name="c.materialId" des="" reqName="materialId" dtype="ch" oper="=" lg="AND" bt="" />
    </where>
  	<select>
      <f name="c.recordId" alias="" ctype="" />
      <f name="c.materialId" alias="" ctype="" />
      <f name="format((SUM(REPLACE(a.price,',','') * c.weight / c.convertUnit)+b.laborPrice + b.powerPrice) * ( 1+b.managementCostRate /100),4)" alias="newPrice" ctype="" />
  </select>
    <!-- 条件可以获得系统变量值 '@:来源session,#来源 2016/1/8未实现' -->
    <!-- $：由程序根据where句的情况追加where关键字' 无$：程序不负责追加where关键字，在from句中需写入where关键字 -->
   <from>b_zzrawmaterial c LEFT JOIN b_purchaseplan a ON a.materialId = c.rawMaterialId LEFT JOIN b_zzmaterialprice b ON b.materialId = c.materialId</from>
    <orderby>c.materialId,c.rawMaterialId</orderby>
    <groupby>c.materialId </groupby>
    <having></having>
	<power></power>    
  </query>
<query>
    <pool></pool>    
    <name value="getPurchaseDetail" />
    <where>
    	<w name="a.YSId" des="" reqName="YSId" dtype="ch" oper="=" lg="AND" bt="" />
    </where>
  	<select>
      <f name="a.recordId" alias="" ctype="" />
      <f name="a.YSId" alias="" ctype="" />
      <f name="a.materialId" alias="" ctype="" />
      <f name="a.materialName" alias="" ctype="" />
      <f name="a.unit" alias="" ctype="计量单位" />
      <f name="a.price" alias="" ctype="" />
      <f name="a.supplierId" alias="" ctype="" />
      <f name="IFNULL(d.price, 0)" alias="lastPrice" ctype="" />
      <f name="d.supplierId" alias="lastSupplierId" ctype="" />
      <f name="IFNULL(b.minPrice, 0)" alias="minPrice" ctype="" />
      <f name="b.minSupplierId" alias="" ctype="" />
      <f name="a.orderQuantity" alias="" ctype="" />
      <f name="a.quantity" alias="" ctype="" />
      <f name="a.priceStatus" alias="" ctype="" />
  </select>
    <!-- 条件可以获得系统变量值 '@:来源session,#来源 2016/1/8未实现' -->
    <!-- $：由程序根据where句的情况追加where关键字' 无$：程序不负责追加where关键字，在from句中需写入where关键字 -->
   <from>v_purchaseplan a LEFT JOIN v_supplierprice2 b ON a.materialId = b.materialId LEFT JOIN b_pricesupplier d ON a.materialId = d.materialId AND a.supplierId = d.supplierId AND d.deleteFlag = '0' </from>
    <orderby>a.materialId</orderby>
    <groupby></groupby>
    <having></having>
	<power></power>    
  </query>
</root>