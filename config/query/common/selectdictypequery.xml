<?xml version="1.0" encoding="UTF-8"?>
<root>
  <query>
    <pool></pool>     
    <name value="selectdictypequery_search" />
    <where>
    </where>
    <select>
      <f name="a.DictypeID" alias="" ctype="" />
      <f name="a.DicTypeName" alias="" ctype="" />
    </select>
    <!-- 条件可以获得系统变量值 '@:来源session,#来源 2016/1/8未实现' -->
    <!-- $：由程序根据where句的情况追加where关键字' 无$：程序不负责追加where关键字，在from句中需写入where关键字 -->
    <from>s_DicType as a  WHERE a.DeleteFlag = '0' AND a.EnableFlag = '0' </from>
    <orderby>A.SortNo, A.DicTypeID</orderby>
    <groupby></groupby>
    <having></having>  
  </query>

</root>